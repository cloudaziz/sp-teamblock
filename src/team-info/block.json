{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/team-info",
	
	"version": "0.1.0",
	"title": "Team Info Block",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example static block scaffolded with Create Block tool.",
	"supports": {
		"html": false
	},
	"keywords": ["team", "info", "teaminfom"],
	"attributes": {
		"title": {
			"type": "string",
			"source": "html",
			"selector": "h3"
		},
		"designation": {
			"type": "string",
			"source": "html",
			"selector": "div"
		},
		"aboutme": {
			"type": "string",
			"source": "html",
			"selector": "p"
		}
	},
	"textdomain": "team-info",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}